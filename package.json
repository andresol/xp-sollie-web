{
  "name": "code",
  "version": "1.0.0",
  "description": "Sollie's web page ",
  "scripts": {
    "browser-sync": "browser-sync start --proxy \"localhost:8101\" --files \"src/main/resources/assets/**/*.css\" \"src/main/resources/assets/**/*.js\" \"src/main/**/*.ftl\" \"src/main/**/*.xml\" \"src/main/**/*.js\"",
    "build": "concurrently \"npm run build-backend\" \"npm run build-frontend\"",
    "watch-build-backend": "concurrently \"npm run build-backend\"",
    "build-backend": "babel ./src/main/resources -x \".es6\" --out-dir ./src/main/resources",
    "build-frontend": "webpack --mode production",
    "watch": "concurrently \"npm run watch-backend\" \"npm run watch-frontend\"",
    "watch-frontend": "webpack --mode development --watch",
    "watch-backend": "concurrently \"babel ./src/main/resources -x \".es6\" --out-dir ./src/main/resources -w\"",
    "lint-staged": "lint-staged"
  },
  "lint-staged": {
    "*.es6": "eslint",
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.{xml,ftl}": [
      "eclint fix",
      "git add"
    ],
    "*.scss": "stylelint"
  },
  "pre-commit": "lint-staged",
  "keywords": [],
  "author": "Andre Sollie",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-eslint": "8.2.3",
    "babel-loader": "7.1.4",
    "babel-plugin-module-resolver": "3.1.1",
    "babel-plugin-syntax-dynamic-import": "6.18.0",
    "babel-plugin-transform-object-assign": "6.22.0",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-polyfill": "6.26.0",
    "babel-preset-env": "1.6.1",
    "babel-preset-react": "6.24.1",
    "browser-sync": "2.23.7",
    "concurrently": "3.5.1",
    "copy-webpack-plugin": "4.5.1",
    "css-loader": "0.28.11",
    "cssnano": "3.10.0",
    "eclint": "2.7.0",
    "eslint": "4.19.1",
    "eslint-config-airbnb": "16.1.0",
    "eslint-config-airbnb-base": "12.1.0",
    "eslint-import-resolver-babel-module": "4.0.0",
    "eslint-plugin-import": "2.11.0",
    "eslint-plugin-jsx-a11y": "6.0.3",
    "eslint-plugin-react": "7.7.0",
    "file-loader": "1.1.11",
    "lint-staged": "7.0.5",
    "mini-css-extract-plugin": "0.4.0",
    "node-sass": "4.9.0",
    "optimize-css-assets-webpack-plugin": "4.0.1",
    "postcss-critical-split": "2.5.1",
    "postcss-loader": "2.1.4",
    "postcss-normalize": "4.0.0",
    "postcss-pipeline-webpack-plugin": "^4.0.0",
    "pre-commit": "1.2.2",
    "resolve-url-loader": "2.3.0",
    "sass-loader": "7.0.1",
    "scrollreveal": "^3.4.0",
    "style-loader": "0.21.0",
    "stylelint": "9.2.0",
    "stylelint-config-standard": "18.2.0",
    "stylelint-scss": "3.0.1",
    "stylelint-webpack-plugin": "0.10.4",
    "uglifyjs-webpack-plugin": "1.2.5",
    "webpack": "4.6.0",
    "webpack-cli": "2.0.15",
    "webpack-glob-entries": "1.0.1"
  },
  "dependencies": {
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "jquery": "^3.3.1",
    "jquery.easing": "^1.4.1",
    "magnific-popup": "^1.1.0",
    "normalize.css": "^7.0.0"
  }
}
